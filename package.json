{
  "name": "family-office-tracker",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace @family-office-tracker/web",
    "build": "npm run build --workspace @family-office-tracker/web",
    "start": "npm run preview --workspace @family-office-tracker/web",
    "preview": "npm run preview --workspace @family-office-tracker/web",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "node --test",
    "test:watch": "node --test --watch",
    "help": "node apps/cli/src/cli.js help",
    "import": "node apps/cli/src/cli.js import",
    "plan": "node apps/cli/src/cli.js plan",
    "categorize": "node apps/cli/src/cli.js categorize",
    "export": "node apps/cli/src/cli.js export",
    "report": "node apps/cli/src/cli.js report"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^20.17.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  }
}
